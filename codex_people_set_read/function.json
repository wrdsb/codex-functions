{
    "bindings": [
      {
        "name": "req",
        "type": "httpTrigger",
        "webHookType": "genericJson",
        "direction": "in",
        "methods": [
          "post"
        ]
      },
      {
        "name": "codexRecordIn",
        "type": "documentDB",
        "databaseName": "wrdsb-codex",
        "collectionName": "codex-people-sets",
        "id" : "{id}",
        "connection": "wrdsb-codex_DOCUMENTDB",
        "direction": "in"
      },
      {
        "name": "codexBlobOut",
        "type": "blob",
        "path": "people-set-objects/{id}.json",
        "connection": "wrdsbcodex_STORAGE",
        "direction": "out"
      },
      {
        "name": "res",
        "type": "http",
        "direction": "out"
      }
    ],
    "disabled": false
  }
  