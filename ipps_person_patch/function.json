{
  "bindings": [
    {
      "name": "req",
      "type": "httpTrigger",
      "webHookType": "genericJson",
      "direction": "in",
      "methods": [
        "post"
      ]
    },
    {
      "name": "codexIPPSPersonIn",
      "type": "documentDB",
      "databaseName": "wrdsb-codex",
      "collectionName": "ipps-people",
      "id" : "{ein}",
      "connection": "wrdsb-codex_DOCUMENTDB",     
      "direction": "in"
    },
    {
      "name": "codexIPPSPersonOut",
      "type": "documentDB",
      "databaseName": "wrdsb-codex",
      "collectionName": "ipps-people",
      "createIfNotExists": true,
      "connection": "wrdsb-codex_DOCUMENTDB",     
      "direction": "out"
    },
    {
      "name": "skylineEvents",
      "type": "eventHub",
      "path": "events",
      "connection": "wrdsb-skyline_EVENTHUB",
      "direction": "out"
    },
    {
      "name": "res",
      "type": "http",
      "direction": "out"
    }
  ],
  "disabled": false
}
